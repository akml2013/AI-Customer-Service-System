<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qa.dao.AccessLogDao">
    <resultMap id="accessLogResultMap" type="com.session.bean.SessionAccessLog">
        <id property="id" column="id"/>
        <result property="sessionId" column="session_id"/>
        <result property="userId" column="user_id"/>
        <result property="accessTime" column="access_time"/>
        <result property="operation" column="operation"/>
    </resultMap>

    <insert id="createAccessLog" parameterType="com.session.bean.SessionAccessLog">
        INSERT INTO session_access_log (session_id, user_id, access_time, operation)
        VALUES (#{sessionId}, #{userId}, #{accessTime}, #{operation})
    </insert>
</mapper>